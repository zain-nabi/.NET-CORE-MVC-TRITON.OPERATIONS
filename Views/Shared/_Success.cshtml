@{
    ViewData["Title"] = "Success";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<main id="content" role="main" class="main">
    <!-- Content -->
    <div class="content container-fluid">
        <div class="row justify-content-sm-center text-center py-10">
            <div class="col-sm-7 col-md-5">
                <img class="img-fluid mb-5" src="~/front-dashboard-v1.1/dist/assets/svg/illustrations/calendar.svg" alt="Image Description" style="max-width: 21rem;">

                <h1>@Html.Raw(ViewData["Header"])</h1>
                <p>@Html.Raw(ViewData["Message"])</p>

                <a class="btn btn-primary" href="@ViewData["Url"]">Continue</a>
            </div>
        </div>
        <!-- End Row -->
    </div>
    <!-- End Content -->
</main>